<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Index</title>
  </head>
  <body>
    <h1>Messages</h1>
    <ul>
      <% messages.forEach((message) => { %>
      <li><%= message.text %></li>
      <li><%= message.user %></li>
      <li><%= message.added %></li>
      <br />

      <% }); %>
    </ul>
  </body>
</html>
